<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    android:background="#ff000000"
    tools:context="com.armagefinder.morriss.armagefinder.ResultsActivity"
    android:paddingRight="10dp"
    android:paddingLeft="10dp">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/mac"
        android:src= "@mipmap/mac">
    </ImageView>
    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context=".ResultsActivity"
        android:id="@+id/ResultsTable"
        android:layout_centerInParent="true">

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/headerTableRow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/headerTextView"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:typeface="serif"
                android:textSize="18sp"
                android:text="Your arm is this many years old: "
                android:gravity="center"
                android:textColor="#ffd700">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/armAgeRow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/ArmAge"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:typeface="serif"
                android:textSize="18sp"
                android:text="(RESULT)"
                android:gravity="center"
                android:textColor="#000000">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/diagnosisHeaderTableRow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/diagnosisHeaderTextView"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:typeface="serif"
                android:textSize="18sp"
                android:text="Diagnosis: "
                android:gravity="center"
                android:textColor="#ffd700">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/blankRow6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/blankTextViewRow6"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp"
                android:text="">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/diagnosisRow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/Diagnosis"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:typeface="serif"
                android:textSize="18sp"
                android:text="(DIAGNOSIS)"
                android:gravity="center"
                android:textColor="#000000">
            </TextView>
        </LinearLayout>


    </TableLayout>
</RelativeLayout>
